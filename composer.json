{
    "name": "symplify/multi-coding-standard",
    "description": "Allows to use multiple coding standards with ease.",
    "license": "MIT",
    "authors": [
        { "name": "Tomáš Votruba", "homepage": "http://tomasvotruba.cz" }
    ],
    "autoload": {
        "psr-4": {
            "Symplify\\MultiCodingStandard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\MultiCodingStandard\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^5.6|^7.0",
        "symfony/console": "^2.8|^3.0",
        "symfony/dependency-injection": "^2.8|^3.0",
        "symfony/process": "^2.8|^3.0",
        "nette/di": "^2.3",
        "nette/bootstrap": "^2.3",
        "tracy/tracy": "^2.3",
        "squizlabs/php_codesniffer": "^2.6",
        "friendsofphp/php-cs-fixer": "^1.11",
        "nette/utils": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "symplify/coding-standard": "^0.2"
    },
    "bin": ["bin/multi-cs"]
}
