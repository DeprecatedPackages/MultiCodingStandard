{
    "name": "symplify/multi-coding-standard",
    "description": "Allows to use multiple coding standards with ease.",
    "license": "MIT",
    "authors": [
        { "name": "Tomáš Votruba", "homepage": "http://tomasvotruba.cz" }
    ],
    "require": {
        "php": "^7.0",
        "symplify/php7_codesniffer": "^0.6",
        "friendsofphp/php-cs-fixer": "^1.12",
        "squizlabs/php_codesniffer": "3.0.x-dev as 2.6",
        "symfony/console": "3.2.x-dev as 3.1",
        "symfony/dependency-injection": "^3.0",
        "symfony/process": "^3.0",
        "nette/di": "^2.4",
        "nette/bootstrap": "^2.4",
        "nette/utils": "^2.4",
        "tracy/tracy": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "symplify/coding-standard": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\MultiCodingStandard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\MultiCodingStandard\\Tests\\": "tests"
        }
    },
    "bin": ["bin/multi-cs"]
}
