require_once __DIR__.'/bootstrap.php';

$containerFactory = new \Symplify\MultiCodingStandard\DI\ContainerFactory();
$container = $containerFactory->create();


/** @var Symplify\MultiCodingStandard\Console\MultiCodingStandardApplication $application */
$application = $container->getByType(Symplify\MultiCodingStandard\Console\MultiCodingStandardApplication::class);
$application->setDefaultCommand('run', true);
$application->run();

